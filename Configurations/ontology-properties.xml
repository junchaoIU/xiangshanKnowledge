<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<!-- 
	本体的配置文件。
	
	作者:Rosahen
	版本:1.0
 -->
<Ontology_Properties>


	<!-- 配置NamedGraph -->
	<Named_Graph>
		
		<!-- 配置默认Graph -->
		<Default_Graph>
			<URI>http://www.owl-ontologies.com/xiangshan_culture_whole</URI>
		</Default_Graph>
		
	</Named_Graph>
	
	<!-- 配置本体根文件的URI -->
	<Root_Ontology_URI>http://www.owl-ontologies.com/XiangshanOnto.owl</Root_Ontology_URI>
	
	<!-- 配置本体文件的URI和文件的路径 -->
	<Ontology_Files>
	
		<Ontology_File>
			<CLASS>所有类</CLASS>
			<URI>http://www.owl-ontologies.com/XiangshanOnto.owl</URI>
			<File_Path>owl/XiangshanOnto.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>艺术</CLASS>
			<URI>http://www.owl-ontologies.com/Art.owl</URI>
			<File_Path>owl/Art.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>职务和称号</CLASS>
			<URI>http://www.owl-ontologies.com/Duty_Title.owl</URI>
			<File_Path>owl/Duty_Title.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>历史事件</CLASS>
			<URI>http://www.owl-ontologies.com/Event.owl</URI>
			<File_Path>owl/Event.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>民俗</CLASS>
			<URI>http://www.owl-ontologies.com/Folk.owl</URI>
			<File_Path>owl/Folk.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>香山人物</CLASS>
			<URI>http://www.owl-ontologies.com/Individual.owl</URI>
			<File_Path>owl/Individual.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>方言</CLASS>
			<URI>http://www.owl-ontologies.com/Localism.owl</URI>
			<File_Path>owl/Localism.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>地点</CLASS>
			<URI>http://www.owl-ontologies.com/Location.owl</URI>
			<File_Path>owl/Location.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>组织机构</CLASS>
			<URI>http://www.owl-ontologies.com/Organization.owl</URI>
			<File_Path>owl/Organization.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>文化遗存</CLASS>
			<URI>http://www.owl-ontologies.com/Relics.owl</URI>
			<File_Path>owl/Relics.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>思想精神文化</CLASS>
			<URI>http://www.owl-ontologies.com/Think.owl</URI>
			<File_Path>owl/Think.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>时间</CLASS>
			<URI>http://www.owl-ontologies.com/Time.owl</URI>
			<File_Path>owl/Time.owl</File_Path>
		</Ontology_File>
		
		<Ontology_File>
			<CLASS>事物关系</CLASS>
			<URI>http://www.owl-ontologies.com/Relation.owl</URI>
			<File_Path>owl/Relation.owl</File_Path>
		</Ontology_File>
		
	</Ontology_Files>
	
</Ontology_Properties>
