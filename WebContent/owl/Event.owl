<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:历史事件="http://www.owl-ontologies.com/Event.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.owl-ontologies.com/Event.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.owl-ontologies.com/Relics.owl"/>
    <owl:imports rdf:resource="http://www.owl-ontologies.com/Organization.owl"/>
  </owl:Ontology>
  <rdfs:Class rdf:about="http://www.owl-ontologies.com/Individual.owl#香山人物"/>
  <owl:Class rdf:ID="社会事件">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="历史事件"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >社会事件</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="自然事件">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#历史事件"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >自然事件</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="政治事件">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#历史事件"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >政治事件</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="军事事件">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >军事事件</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#历史事件"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="文化事件">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >文化事件</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#历史事件"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="教育事件">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#历史事件"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >教育事件</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="经济事件">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >经济事件</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#历史事件"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="科技事件">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >科技事件</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#历史事件"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#历史事件">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >历史事件</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="领导人">
    <rdfs:domain rdf:resource="#历史事件"/>
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/Individual.owl#香山人物"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="被协助方">
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/Individual.owl#香山人物"/>
    <rdfs:domain rdf:resource="#历史事件"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="起始年份">
    <rdfs:domain rdf:resource="#历史事件"/>
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/Time.owl#时间"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="结束年份">
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/Time.owl#时间"/>
    <rdfs:domain rdf:resource="#历史事件"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="发生地点">
    <rdfs:domain rdf:resource="#历史事件"/>
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/Location.owl#地点"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="协助方">
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/Individual.owl#香山人物"/>
    <rdfs:domain rdf:resource="#历史事件"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="相关人物">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#历史事件"/>
          <rdf:Description rdf:about="http://www.owl-ontologies.com/Relation.owl#事件"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/Individual.owl#香山人物"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="结束时间">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:domain rdf:resource="#历史事件"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="别名">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#历史事件"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="起始时间">
    <rdfs:domain rdf:resource="#历史事件"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>
  <历史事件:文化事件 rdf:ID="任全国政协委员">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >郑君里</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑君里"/>
  </历史事件:文化事件>
  <历史事件:政治事件 rdf:ID="携饷入黄埔慰劳卫士队"/>
  <历史事件:文化事件 rdf:ID="被称为_二胡王_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >青年时期，吕文成有“二胡王”之称。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#吕文成"/>
  </历史事件:文化事件>
  <历史事件:文化事件 rdf:ID="任_新民报_美术编辑">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1949年，方成回北京，次年任《新民报》美术编辑，</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#方成"/>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="创办先施公司">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1899年，马应彪筹集资金港币2.5万元，在香港开设先施公司并于1900年正式开业。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="创办了_上海周报_">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >抗战爆发后，韦悫创办了《上海周报》，任总编辑。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="与人合开_广生和_杂货店">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1885年，黄焕南艰苦奋斗，积累资金，与人合开一家名为“广生和”的杂货店。由于经营得法，“广生和”渐成规模，发展成百货商店，他也成为知名侨商。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#黄焕南"/>
  </历史事件:经济事件>
  <历史事件:军事事件 rdf:ID="支援十九路军抗日">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1932年，张惠长组织了一支混合机队前往杭州，支援十九路军抗日。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#张惠长"/>
  </历史事件:军事事件>
  <历史事件:文化事件 rdf:ID="代表工联会参加全港兵乓球锦标赛">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#容国团"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1957年2月，容国团代表工联会参加香港举行的全港兵乓球锦标赛，一举夺得男子团体、男子双打、男子单打三项冠军，从此声名大振。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="建立_芳植记_商号"/>
  <历史事件:政治事件 rdf:ID="开创中华民国"/>
  <历史事件:教育事件 rdf:ID="兴办周崧学校">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#周崧"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1928年，在乡兴办完全小学，命名为周崧学校。1931年至1949年，共办19届，毕业生514人。该校一向免收学杂各费，一切费用由周崧支付。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:经济事件 rdf:ID="投资轮船招商总局">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1884年，唐廷植代唐廷枢暂时管理轮船招商总局，投资轮船招商总局，任局董</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="考入日本绘画专门学院">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1907年，郑锦考入日本美术最高学府——日本绘画专门学院。同年，创作了《娉婷》（被台北故宫博物院收藏）一帧。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑锦"/>
  </历史事件:文化事件>
  <历史事件:教育事件 rdf:ID="兼任北京大学_国立北京高等师范大学和国立女子师范学校教师">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑锦"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1914年，郑锦应国民政府教育部的邀请回到北京。同时，兼任北京大学、国立北京高等师范大学、国立女子师范学校的教师。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="赴日本留学">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1899年，苏曼殊赴日本，在横滨考入华侨办的大同学校，后考入早稻田大学高等预科。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#苏曼殊"/>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="被国民党反动派逮捕">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨匏安"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1931年7月，杨匏安被国民党反动派逮捕，壮烈牺牲。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:教育事件 rdf:ID="去美国留学">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1847年，黄胜与容闳等一起去美国读书。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#黄胜"/>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#容闳"/>
  </历史事件:教育事件>
  <历史事件:军事事件 rdf:ID="创办_图强飞机公司_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1918年，杨仙逸在夏威夷创办“图强飞机公司”。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨仙逸"/>
  </历史事件:军事事件>
  <历史事件:文化事件 rdf:ID="任_人民日报_高级编辑">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1951年，方成任《人民日报》高级编辑。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="被聘为广州先施公司司理">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1913年，同乡马应彪（已在香港广州创办了当地称雄的先施百货公司）素知黄焕南是商业奇才，邀请他出任先施公司董事，再聘为广州先施公司司理，负责上海先施公司的筹建事宜。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="获得全国九城市兵乓球单打冠军">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#容国团"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1958年4月，容国团代表广州参加了全国九城市兵乓球，获得单打冠军。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="前往澳洲谋生">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1873年，黄焕南前往澳洲谋生。
1891年，蔡昌赴前往澳洲谋生。
1892年，郭乐前往澳洲谋生。
1899年，李敏周前往澳洲谋生。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:军事事件 rdf:ID="中法之战"/>
  <历史事件:经济事件 rdf:ID="合开_成号布庄_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >徐润与人合开“成号布庄”。业务从上海到浙赣山区，从长江内河到沿海各埠，甚至伸至日本长崎。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:经济事件 rdf:ID="接任太古洋行第三任买办">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1917年，莫干生接任父亲当上太古洋行第三任买办。洋行华人买办，在历史上，没有一个像太古洋行那样，祖孙三代连续充任的。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="就读两广方言学校">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1903年，郑道实到广州就读于两广方言学校英语科，不久因病辍学返乡。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="开展农民运动">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1925年，韦悫与毛泽东相识于广州农民运动讲习所。两人初次见面，畅谈了一个多小时，受毛启发，在当时任国民党中央农民部长的廖仲恺的帮助下，回到故乡香山县开展农民运动。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
  </历史事件:政治事件>
  <历史事件:经济事件 rdf:ID="集资筹建_中华飞船公司_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1915年，杨著昆响应孙中山“航空救国”号召，以大股东身份与当地华侨集资筹建“中华飞船公司”，准备生产战斗机供革命使用。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="建立清华学堂"/>
  <历史事件:经济事件 rdf:ID="修建大新大厦">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1918年，在广州西堤珠江边，修建规模宏大、拥有12层楼高的大新大厦（今天的南方大厦）。大厦布局新颖，设备齐全，其独特设计为国内罕见，成为当时华南最宏伟最华丽的百货商店。外地游客言称：“不到大新，等于没来广州”。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="任华东大学校长">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1948年夏，韦悫办华东大学（济南解放后迁入 济南），并任校长。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
  </历史事件:教育事件>
  <历史事件:文化事件 rdf:ID="曾任中国电影工作者协会理事">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >郑君里</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑君里"/>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="接任太古洋行第二任买办">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >莫仕扬去世后，莫藻泉接任太古洋行第二任买办。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="积极参加音乐活动">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#吕文成"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1925年，吕文成担任该会沪乐科干事，同时兼上海精武体育会音乐部教员，参加了由上海铁路职工组织的“俭德储蓄会”粤乐队的活动，并担任指导。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:文化事件 rdf:ID="在加州帕沙迪纳大学学习演技"/>
  <历史事件:社会事件 rdf:ID="回到家乡">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1922-1927年，方成回到家乡生活、读书。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#方成"/>
  </历史事件:社会事件>
  <历史事件:政治事件 rdf:ID="捐助兴中会">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#孙眉"/>
  </历史事件:政治事件>
  <历史事件:经济事件 rdf:ID="创办中兴公司">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >周崧长大后前往美国经商，创办中兴公司，因经营有方，成为巨富，是当时旅美华侨中知名的企业家。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:经济事件 rdf:ID="开设有开平煤栈及山海关锦州煤栈">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >郑翼之开设有开平煤栈及山海关锦州煤栈。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="获得博士学位">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1857年，黄宽获得博士学位。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#黄宽"/>
  </历史事件:教育事件>
  <历史事件:社会事件 rdf:ID="成立中华海员工会联合总会"/>
  <历史事件:教育事件 rdf:ID="选择航空专业学习">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨仙逸"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1909年，杨仙逸在美国读大学期间，因十分赞同孙中山的“航空救国”主张，专门选择了航空专业学习。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="出席太平洋教育会议">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1921年6月，孙中山委派他作为革命政府代表，韦悫出席在檀香山举行的太平洋教育会议，并在大会上发表了演说。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
  </历史事件:教育事件>
  <历史事件:文化事件 rdf:ID="创作童话诗_小姑娘与乌猿婆_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1982年，阮章竞写了童话诗《小姑娘与乌猿婆》。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#阮章竞"/>
  </历史事件:文化事件>
  <历史事件:教育事件 rdf:ID="协助各省派遣留美学生">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#唐国安"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1909-1911年，唐国安协助各省直接派遣了3批留美学生。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="驾驶广州号飞机单机环飞全国">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#张惠长"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1928年，为了宣传“航空救国”，张惠长曾驾“广州”号飞机单机环飞全国，在全国引起一股航空热。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:教育事件 rdf:ID="被任命为教育部副部长">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1949年11月，韦悫被任命为教育部副部长。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="应聘担任格致书院汉文总教习">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1898年，钟荣光以名举人身份，应聘担任美国教会势力在广州开办的教会学校——格致书院（岭南大学前身）的汉文总教习。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#钟荣光"/>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="被任为特派专员">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1936年，陈庆云被任为特派专员，处理广东空军北并中央的遗留问题和策动广西空军归并中央。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:文化事件 rdf:ID="大中华百合影业公司改名联华影业公司"/>
  <历史事件:科技事件 rdf:ID="施行国内第一例胚胎截开手术">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#黄宽"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1860年，黄宽施行国内第一例胚胎截开手术。</rdfs:comment>
  </历史事件:科技事件>
  <历史事件:社会事件 rdf:ID="在上海病逝">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1940年12月31日，萧友梅在上海病逝。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#萧友梅"/>
  </历史事件:社会事件>
  <历史事件:社会事件 rdf:ID="被誉为_善翁钟工宇_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >钟工宇笃信基督教，乐善好施，对美国、港澳、广州等国家和地区基督教会附设的慈善机构，如广州芳村孤儿院、盲人院、万善中学和小学，以及对他的家乡的西山学校，桥梁道路的修建，均有捐助，故被誉为“善翁钟工宇”。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#钟工宇"/>
  </历史事件:社会事件>
  <历史事件:教育事件 rdf:ID="到美国接受完全之教育">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1846年9月，黄宽与容闳等人一起，到美国接受完全之教育。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#黄宽"/>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#容闳"/>
  </历史事件:教育事件>
  <历史事件:经济事件 rdf:ID="随父上海谋生">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#吕文成"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >吕文成幼年随父上海谋生。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="入读私塾">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1895年，郑道实入读私塾，爱好读书，天资聪颖，记忆力强。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="被任为中国驻夏威夷第一任领事">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1881年，“华董”升格为中国驻夏威夷领事馆，光绪皇帝钦命陈芳为中国驻夏威夷第一任领事，官居二品。在华商和领事任内，他致力于议订一些保障华人权利的规章，在华人中产生了很大的影响。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#陈芳"/>
  </历史事件:政治事件>
  <历史事件:文化事件 rdf:ID="被梅兰芳认为义女">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1929年，卢燕随母到上海，寄寓在京剧大师梅兰芳家，被梅认为义女。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#卢燕"/>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="开办_润立生茶号_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1860年，徐润独资在温州开办“润立生茶号”。业务从上海到浙赣山区，从长江内河到沿海各埠，甚至伸至日本长崎。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:政治事件 rdf:ID="担任国大代表">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1936年以后，陈庆云担任过国大代表。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#陈庆云"/>
  </历史事件:政治事件>
  <历史事件:文化事件 rdf:ID="担任中国作家协会党总支书记_党组成员_中国作协青年作家工作委员会主任">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1955年4月，阮章竞担任中国作家协会党总支书记、党组成员、中国作协青年作家工作委员会主任。这是他的童话诗歌创作高峰。《金色的海螺》，获全国儿童文学首次评奖的一等奖。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#阮章竞"/>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="提出创建国家银行计划及建议组建全国铁路公司">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >容闳</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="任北京作家协会主席_名誉主席">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#阮章竞"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >新中国成立后，阮章竞任北京作家协会主席、名誉主席。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:政治事件 rdf:ID="遭通缉追捕">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1911年，韦悫因参加刺杀广州将军凤山的行动，遭到清廷两广总督张鸣岐的通缉追捕。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
  </历史事件:政治事件>
  <历史事件:政治事件 rdf:ID="不赞成关于粤都督的提议"/>
  <历史事件:文化事件 rdf:ID="世界兵乓球锦标赛冠军"/>
  <历史事件:经济事件 rdf:ID="成为茂宜岛首富"/>
  <历史事件:文化事件 rdf:ID="被选为全国文联委员_美协常务理事">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#古元"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1949年7月，古元出席全国第一届文代会，被选为全国文联委员、美协常务理事。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:文化事件 rdf:ID="组织广州早期音乐社团_中华音乐会_">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑志声"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >郑志声组织广州早期的音乐社团“中华音乐会”，结识了岭南乐队的领导人冼星海。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:文化事件 rdf:ID="主演_最佳国产无声片__人生_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1934年，阮玲玉主演了《人生》一片，阮在片中扮演了一名“路柳墙花”的角色，从少女到老妪。该片当年被评为“最佳国产无声片”。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#阮玲玉"/>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="赴香港经商">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1860年，莫仕扬赴香港经商。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="书法作品获选日本特选奖状">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >新中国成立后，刘逸生的书法作品获选日本书艺院第51回国际文化交流书道展的特选奖状。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#刘逸生"/>
  </历史事件:文化事件>
  <历史事件:文化事件 rdf:ID="参加中国艺术社">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1943年，郑君里参加中国艺术社，从演员转当导演。导演过《一江春水向东流》、《乌鸦与麻雀》等著名影片。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑君里"/>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="被聘为太古洋行帮买办">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1928年，莫应溎被聘为太古洋行的帮买办（副买办），主管糖厂。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:政治事件 rdf:ID="参与飞行全国活动">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨官宇"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1928年冬，杨官宇参与了飞行全国活动。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:政治事件 rdf:ID="任孙中山秘书">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1921年5月5日，孙中山在广州就任非常大总统，韦兼任孙中山秘书。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
  </历史事件:政治事件>
  <历史事件:军事事件 rdf:ID="任大元帅上校"/>
  <历史事件:文化事件 rdf:ID="结识共产党人和越南领袖">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1924年1月，韦悫认识了共产党人张太雷、恽代英和越南领袖胡志明。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:教育事件 rdf:ID="在东京帝国大学与东京音乐学院毕业">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1909年，萧友梅从东京帝国大学与东京音乐学院毕业。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#萧友梅"/>
  </历史事件:教育事件>
  <历史事件:军事事件 rdf:ID="北伐战争"/>
  <历史事件:政治事件 rdf:ID="被迫辞去中央组织部长一职">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨匏安"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1926年春，蒋介石压迫共产党和左派，时任中央组织部长的杨匏安被迫辞职，后仍在广州做统战工作。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:教育事件 rdf:ID="培英中学">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1929年，马应彪在任中山模范县训政委员兼财务组主任时所为。</rdfs:comment>
    <历史事件:协助方 rdf:resource="http://www.owl-ontologies.com/Individual.owl#马应彪"/>
  </历史事件:教育事件>
  <历史事件:经济事件 rdf:ID="创办永生公司">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1884年，马应彪在澳洲悉尼，创办永生公司</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:社会事件 rdf:ID="因精神分裂症不治去世">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >70年代初，梁白波因精神分裂症不治去世。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#梁白波"/>
  </历史事件:社会事件>
  <历史事件:经济事件 rdf:ID="成立华美银行">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1915年，钟工宇同唐雄等人集资20万元成立华美银行，担任董事兼司库。至1935年易名中美银行，继续营业。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:政治事件 rdf:ID="作为共产党秘密党员">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1943年，韦悫重新入党，长期为秘密党员。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:经济事件 rdf:ID="任董事兼首任司理">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#黄焕南"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1917年10月20日，国内第一家中资百货大楼先施公司在上海正式开张。黄焕南任董事兼首任司理。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="格致书院更名为岭南学堂">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1900年，格致书院更名为岭南学堂。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:社会事件 rdf:ID="组织_阳和会馆_">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#唐廷植"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1849年，唐廷植赴美国旧金山，在美国排华事件中与当地官员交涉，维护华人利益，成为知名人士，被当地华工、华商选为旧金山华商公所的总董。他热心为侨胞办事，组织“阳和会馆”，团结侨胞，被公认为早期旅美的华侨领袖。</rdfs:comment>
  </历史事件:社会事件>
  <历史事件:经济事件 rdf:ID="创办永安公司">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1907年，郭乐在香港创办永安百货公司。营业额迅速上升，不但获得了巨额利润，而且积累了丰富的经营管理经验。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:政治事件 rdf:ID="广州起义失败"/>
  <历史事件:经济事件 rdf:ID="改组招商总局">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1872年，徐润受李鸿章委派改组招商总局，短期内将资产从不足20万两扩至100万两。后来又一手筹划以220万两收购美国旗昌轮船公司全部家产，使招商总局由开办时的4艘轮船增加至26艘，码头、栈房遍及长江沿岸和沿海各口岸共21处。这是招商总局的发展史，也是中国航运发展史上的一个重要转折。中国轮船吨位由此猛增至400万吨，占进出口中外船只总数的36.7%，从而奠定了中国航运事业的基础。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="重组巨型魔术团">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1951年，张慧冲重组张慧冲巨型魔术团，任团长兼演员。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#张慧冲"/>
  </历史事件:文化事件>
  <历史事件:政治事件 rdf:ID="参加开国大典">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1949年10月，韦悫参加开国大典。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:军事事件 rdf:ID="创办广东飞机制造厂">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨仙逸"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1923年，杨仙逸担任航空局局长，接着他又创办广东飞机制造厂，并任厂长，数月后制成一架飞机，孙中山为表彰他的卓越功勋，亲笔书写“志在冲天”的横额送给他。</rdfs:comment>
  </历史事件:军事事件>
  <历史事件:教育事件 rdf:ID="在上海学习英文">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#苏曼殊"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1897年，苏曼殊到上海，学习英文。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="成为岭南大学校董">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#钟荣光"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >原来设立在纽约的董事局则改为岭南大学美国基金委员会。钟荣光成为岭南大学校董。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="协助容闳挑选120名幼童赴美国留学">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#徐润"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1872年起，徐润受清廷委托，协助容闳挑选120名幼童赴美国留学。其中94人学成回国。且均担任要职。如近代中国铁路之父詹天佑，清代爱国将领邓世昌，民国第一任内阁总理唐绍仪，清华学校（清华大学前身）首任校长唐国安等。</rdfs:comment>
    <历史事件:被协助方 rdf:resource="http://www.owl-ontologies.com/Individual.owl#容闳"/>
    <历史事件:协助方 rdf:resource="http://www.owl-ontologies.com/Individual.owl#徐润"/>
  </历史事件:教育事件>
  <历史事件:文化事件 rdf:ID="开办_绍祥字号_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1859年起，徐润先后与他人合作，开办“绍祥字号”</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#徐润"/>
  </历史事件:文化事件>
  <历史事件:教育事件 rdf:ID="捐资兴建马应彪招待所">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1929年，马应彪在任中山模范县训政委员兼财务组主任时所为。马应彪招待所（现为中山大学港澳珠江三角洲研究中心用房）</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#马应彪"/>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="赴上海美术专门学校深造">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >唐涤生曾赴上海美术专门学校深造</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#唐涤生"/>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="提出三民主义">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#孙中山"/>
  </历史事件:政治事件>
  <历史事件:文化事件 rdf:ID="在利马创办_通惠局_"/>
  <历史事件:教育事件 rdf:ID="岭南大学增办农科">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1917年，岭南大学增办农科。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="_四一二_反革命政变"/>
  <历史事件:教育事件 rdf:ID="岭南学堂迁往广州康乐村">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1904年，岭南学堂迁往广州康乐村。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:政治事件 rdf:ID="任推广普通话工作委员会委员">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1956年，韦悫任推广普通话工作委员会委员</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:文化事件 rdf:ID="被委任为万国禁烟会议中国代表">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#唐国安"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1909年，唐国安被委任为上海召开的万国禁烟会议的中国代表。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:经济事件 rdf:ID="开辟国货商场">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1934年秋天，郭乐在永安商场4楼开辟了国货商场，代办国货寄售业务。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="修完哲学博士全部课程">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1920年，韦卓民修完哲学博士的全部课程。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦卓民"/>
  </历史事件:教育事件>
  <历史事件:文化事件 rdf:ID="任职国立歌剧院和中华交响乐团">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑志声"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >抗战爆发后，郑志声毅然回国效力。到重庆任职于国立歌剧院和中华交响乐团，是当年名望最高的指挥家。其创作的《满江红》、歌剧《郑成功》等，堪称传世佳作。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:政治事件 rdf:ID="被撤销中央监察委员职务">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1927年春，杨匏安到武汉参加中共五大，当选为中央监察委员。同年11月，因谭平山将其列为“第三党”成员，被撤销中央监察委员职务，并给留党察看处分。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨匏安"/>
  </历史事件:政治事件>
  <历史事件:社会事件 rdf:ID="移民美国纽约">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#陈庆云"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1966年，陈庆云退休后移民美国纽约。</rdfs:comment>
  </历史事件:社会事件>
  <历史事件:文化事件 rdf:ID="到省体校集训">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1979年，江嘉良成为广东队主力，后成为国家队队员。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:社会事件 rdf:ID="任中华会馆主席和慈善公益委员会委员长">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#钟工宇"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >钟工宇热心华侨及国内公益事业，任中华会馆主席多年。1912年中华商会成立，内设慈善公益委员会，钟任委员长。</rdfs:comment>
  </历史事件:社会事件>
  <历史事件:教育事件 rdf:ID="于英国毕业">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#黄宽"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1855年，黄宽毕业。获金牌等奖状及医学学士学位。</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:经济事件 rdf:ID="被扣于香港追索赔款"/>
  <历史事件:经济事件 rdf:ID="_芳植记商号_成为檀香山最著名华人商号">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1870年，“芳植记商号”成为檀香山最著名的华人商号，成为檀香山八大企业之一。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:经济事件 rdf:ID="组成先施股份有限公司">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1909年，马应彪组成先施股份有限公司。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:政治事件 rdf:ID="聆听革命演讲">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#杨仙逸"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1903年，杨仙逸聆听孙中山先生在檀香山向华侨作宣传革命的演讲，心中点燃了为中国民主革命而战斗的火焰。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:经济事件 rdf:ID="先施广州分行营业停顿">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1941年，香港沦陷，先施总行、广州分行营业停顿。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:经济事件 rdf:ID="被提升为上海怡和洋行总买办">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1863年，唐廷枢被提升为上海怡和洋行总买办。对于英商怡和洋行来说，“唐廷枢简直成了它能获得华商支持的保证”。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:文化事件 rdf:ID="导演并主演_水落石出_">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#张慧冲"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1924年,张慧冲导演并主演的《水落石出》，是第一部打入外国人控制的电影院的中国影片。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:教育事件 rdf:ID="兼任文华书院中学部校长">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >韦卓民倾心于西方哲学与中国古代哲学的研究，并兼任中学部校长，同时修读研究生课程。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦卓民"/>
  </历史事件:教育事件>
  <历史事件:经济事件 rdf:ID="创办太古轮船公司"/>
  <历史事件:教育事件 rdf:ID="提议将北大音乐研究会改组为北京大学附设音乐传习所">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1922年8月，经萧友梅提议，北大音乐研究会改组为北京大学附设音乐传习所。校长蔡元培兼任所长，萧友梅作教务主任负责实际工作。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#萧友梅"/>
  </历史事件:教育事件>
  <历史事件:社会事件 rdf:ID="随父在上海谋生">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >幼年吕文成随父上海谋生。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#吕文成"/>
  </历史事件:社会事件>
  <历史事件:文化事件 rdf:ID="任中央新闻摄影局美术研究室副主任">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1950年，古元到北京任中央新闻摄影局美术研究室副主任。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#古元"/>
  </历史事件:文化事件>
  <历史事件:政治事件 rdf:ID="任苏皖边区政府副主席">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1945年10月，至1946年12月，韦悫任苏皖边区政府副主席。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦悫"/>
  </历史事件:政治事件>
  <历史事件:社会事件 rdf:ID="病逝纽约">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1981年，陈庆云病逝纽约。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#陈庆云"/>
  </历史事件:社会事件>
  <历史事件:社会事件 rdf:ID="到俄了解十月革命胜利情况"/>
  <历史事件:经济事件 rdf:ID="代表当时积极民族主义经济思潮">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑观应"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >郑观应代表当时积极的民族主义的经济思潮。认为中国应该像西方各国一样大办商业，“中西可共之利，思何以筹之；中国自有之利，思何以扩之；西人独擅之利，思何以分之”。
其商业思想可分为商战论、商道论、营销论、成本论、管理论。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:政治事件 rdf:ID="组织_香港华人救伤队_">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1932年1月28日，日本侵略军攻占上海时，莫应溎义愤填膺，曾组织一支由75人组成的“香港华人救伤队”，自任队长，捐献了一部救护车，携带药品及医疗器械亲赴上海抢救抗日部队。还募捐几万港元转送19路军上海办事处，支援残疾军人教养院。“</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:文化事件 rdf:ID="任中共包头钢铁公司党委委员_宣传部长">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#阮章竞"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1956年11月至1959年12月，阮章竞任中共包头钢铁公司党委委员、宣传部长。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:文化事件 rdf:ID="被推选中国版画家协会名誉主席">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#古元"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1995年，古元被推选为中国版画家协会名誉主席。</rdfs:comment>
  </历史事件:文化事件>
  <历史事件:政治事件 rdf:ID="帮助革命部队脱险">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1924年，莫应溎帮助叶剑英率领的革命部队脱险。受地下党员之托，向香港警务处长交涉，使被扣香港长洲的革命部队脱险。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:政治事件 rdf:ID="参加上海职业界革命活动">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1936年，阮章竞参加上海职业界革命活动，这时期创作了小说《林肯少年时候的故事》并创作了第一首自由体诗《故乡》，发表在上海《青春》上。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#阮章竞"/>
  </历史事件:政治事件>
  <历史事件:社会事件 rdf:ID="因心脏病病逝于香港">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#钟荣光"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1942年1月，钟荣光因心脏病病逝于香港。</rdfs:comment>
  </历史事件:社会事件>
  <历史事件:经济事件 rdf:ID="创办图强飞机公司"/>
  <历史事件:政治事件 rdf:ID="广州起义">
    <历史事件:别名 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >黄花岗起义</历史事件:别名>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1911年4月27日，广州黄花岗之役，因缺乏群众基础，组织不够严密而失败，但革命党人前赴后继，英勇战斗，给清政府以沉重打击，给全国人民以极大鼓舞，在全国引起了巨大震动。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:社会事件 rdf:ID="创办新民启智会"/>
  <历史事件:教育事件 rdf:ID="翻译_判断力批判___康德哲学原著选读_等文献">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >韦卓民的主要译著有：康德的《判断力批判》、华特生的《康德哲学原著选读》等，译文严密可信，通俗流畅，独具风格，在翻译界甚有影响。他的关于康德哲学译著，一直是我国研究康德哲学的学者必读的文献资料。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#韦卓民"/>
  </历史事件:教育事件>
  <历史事件:文化事件 rdf:ID="离职休养">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1986年，方成离职休养。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#方成"/>
  </历史事件:文化事件>
  <历史事件:政治事件 rdf:ID="到太湖一带进行抗日宣传"/>
  <历史事件:政治事件 rdf:ID="任南京航空署署长">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#张惠长"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1929年，张惠长任南京航空署署长。</rdfs:comment>
  </历史事件:政治事件>
  <历史事件:教育事件 rdf:ID="担任国立音乐专科学校校长">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1929年8月19日，萧友梅担任国立音乐专科学校校长。经过数年坚持不懈的努力，终于使国立音专成为具有相当规模与国际水准的中国最高音乐学府。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#萧友梅"/>
  </历史事件:教育事件>
  <历史事件:经济事件 rdf:ID="在香港开设信息庄">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1894年，马应彪在香港开设信息庄及永昌金山庄，开办侨汇兼经销进出口生意。</rdfs:comment>
  </历史事件:经济事件>
  <历史事件:教育事件 rdf:ID="被任为国立北平美术专科学校第一任校长">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1918年4月15日，中国第一所正式立案的“国立北平美术专科学校”成立，郑锦被任命为第一任校长。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#郑锦"/>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="广东国画座谈会负责人">
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#方人定"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >方人定</rdfs:comment>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="荣获捐资兴学一等奖">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1934年，国民政府教育部鉴于周崧个人捐出巨款建校，颁发捐资兴学一等奖。</rdfs:comment>
    <历史事件:相关人物 rdf:resource="http://www.owl-ontologies.com/Individual.owl#周崧"/>
  </历史事件:教育事件>
  <历史事件:教育事件 rdf:ID="毕业于武汉大学化学系">
